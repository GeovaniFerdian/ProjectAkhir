        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        />
    <View   android:id="@+id/_bg__menu_utama_ek2"
        android:background="@drawable/_bg__menu_utama_ek2_shape"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp"
        android:alpha="0.79"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        />
    <ImageView android:id="@+id/button_back1"
        android:src="@drawable/desain_tanpa_judul__4__1"
        android:contentDescription="desain_tanpa_judul__4__1"
        android:layout_marginStart="12dp"
        android:layout_marginTop="15dp"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        />
    <ImageView
        android:id="@+id/petunjuk"
        android:layout_width="237dp"
        android:layout_height="71dp"
        android:layout_marginTop="30dp"
        android:src="@drawable/ptunjuk"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        />
    <View   android:id="@+id/rectangle_7"
        android:background="@drawable/rectangle_7_shape"
        android:alpha="0.80000001192093"
        android:layout_marginTop="40dp"
        android:layout_width="349dp"
        android:layout_height="105dp"
        app:layout_constraintTop_toBottomOf="@id/petunjuk"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <View   android:id="@+id/rectangle_8"
        android:background="@drawable/rectangle_7_shape"
