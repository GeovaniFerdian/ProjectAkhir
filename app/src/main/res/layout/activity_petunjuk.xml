        android:text="@string/text_ek3_string"
        app:layout_constraintTop_toTopOf="@id/rectangle_10"
        app:layout_constraintStart_toStartOf="@id/rectangle_10"
        app:layout_constraintBottom_toBottomOf="@id/rectangle_10"
        />

    <View   android:id="@+id/rectangle_11"
        android:background="@drawable/rectangle_7_shape"
        android:alpha="0.80000001192093"

        android:layout_marginTop="50dp"
        android:layout_width="349dp"
        android:layout_height="105dp"
        app:layout_constraintTop_toBottomOf="@id/rectangle_10"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <TextView android:id="@+id/text_ek4"
        android:textStyle="bold"
        android:textSize="17sp"
        android:textColor="@color/text_color"
        android:layout_width="343dp"
        android:layout_height="101dp"
        android:text="@string/text_ek4_string"
        app:layout_constraintTop_toTopOf="@id/rectangle_11"
        app:layout_constraintStart_toStartOf="@id/rectangle_11"
        app:layout_constraintBottom_toBottomOf="@id/rectangle_11"
        />

</androidx.constraintlayout.widget.ConstraintLayout>